<!DOCTYPE html>
<html ng-app='ttt'>
<head>
<meta charset=utf-8 />
<title>JS Bin</title>
<link href='/ttt.css' rel='stylesheet'>
<script src='/jquery-1.10.2.min.js'></script>
<script src='/angular.min.js'></script>
<script src='/ttt.js'></script>
</head>
<body ng-controller='TTTCtrl'>
  <h1>Tic Tac Toe</h1>
  <table>
  	<tr id='row_1'>
  		<td id='cell-1'>{{ cell1 }}</td>
  		<td id='cell-2'>{{ cell2 }}</td>
  		<td id='cell-3'>{{ cell3 }}</td>
  	</tr>
  	<tr id='row_2'>
  		<td id='cell-4'>{{ cell4 }}</td>
  		<td id='cell-5'>{{ cell5 }}</td>
  		<td id='cell-6'>{{ cell6 }}</td>
  	</tr>
  	<tr id ='row_3'>
  		<td id='cell-7'>{{ cell7 }}</td>
  		<td id='cell-8'>{{ cell8 }}</td>
  		<td id='cell-9'>{{ cell9 }}</td>
  	</tr>
  </table>
</body>
</html>
